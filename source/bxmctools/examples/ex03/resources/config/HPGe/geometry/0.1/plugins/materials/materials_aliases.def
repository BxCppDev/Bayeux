# -*- mode: conf-unix; -*-
# List of multi-properties (datatools::multi_properties):

#@description The list of aliases for standard materials
#@key_label  "name"
#@meta_label "type"

# All materials named "std::xxx" are provided by the bayeux/materials
# library. We use here some convenience aliases for the materials used
# in the geometry setup. This enables to easily change the effective
# material of a logical volume by changing this file, without modifying
# the geometry definition files.

[name="air" type="alias"]
#@description The alias for air
material : string = "std::air"

[name="aluminium" type="alias"]
#@description The alias for aluminium
material : string = "std::aluminium"

[name="copper" type="alias"]
#@description The alias for copper
