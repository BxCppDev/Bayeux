[name="gamma_0-3MeV_flat" type="genbb::single_particle_generator"]
#@config Gamma with flat ranged energy between 0 and 3 MeV

#@description Set the logging threshold
logging.priority   : string = "fatal"

# #@description Set the PRNG seed with an arbitrary value
# seed               : integer = 314159

#@description Generate gamma particles
particle_name      : string = "gamma"

#@description Randomization (4 pi) of the direction
emission_direction : string = "randomized"

#@description Use MeV as the default energy unit
energy_unit        : string = "MeV"

#@description Energy range mode
mode               : string = "energy_range"

#@description Set the minimum kinetic energy
min_energy         : real as energy = 0.0 MeV

#@description Set the maximum kinetic energy
max_energy         : real as energy = 3.0 MeV

