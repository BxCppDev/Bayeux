# -*- mode: conf-unix; -*-
# List of multi-properties (datatools::utils::multi_properties):

#@description A list of common materials
#@key_label   "name"
#@meta_label  "type"


[name="std::vacuum" type="material"]
#@description Vacuum material
density        : real   = 1.0e-15
density.unit   : string = "mg/cm3"
state          : string = "gas"
composition.mode : string   = "fraction_mass"
composition.names         : string [1]  = "Hydrogen"
composition.fraction_mass : real   [1]  =  1.0


[name="std::air" type="material"]
#@description Air material
density        : real   = 1.2931
density.unit   : string = "mg/cm3"
state          : string = "gas"
composition.mode : string   = "fraction_mass"
composition.names         : string [2]  = "Nitrogen" "Oxygen"
composition.fraction_mass : real   [2]  =  0.7        0.3


[name="std::hydrogen_gas" type="material"]
#@description Hydrogen gas
density        : real   = 1.0
density.unit   : string = "mg/cm3"
state          : string = "gas"
composition.mode : string   = "fraction_mass"
composition.names         : string [1]  = "Hydrogen"
composition.fraction_mass : real   [1]  =  1.0


[name="std::copper" type="material"]
