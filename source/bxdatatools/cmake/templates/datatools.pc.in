prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/@CMAKE_INSTALL_LIBDIR@
includedir=${exec_prefix}/@CMAKE_INSTALL_INCLUDEDIR@

Name: @CMAKE_PROJECT_NAME@
Description: The datatools API
URL: https://nemo.lpc-caen.in2p3.fr/wiki/datatools
Version: @datatools_VERSION_STR@
@_datatools_pkgconfig_Requires@
Libs: -L${libdir} @_datatools_pkgconfig_Libs@
Cflags: -I${includedir} @_datatools_pkgconfig_Cflags@
