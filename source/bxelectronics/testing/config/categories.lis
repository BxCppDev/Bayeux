# -*- mode: conf-unix; -*-

#@description The definition of electronics categories
#@key_label  "category"
#@meta_label "type"


[category="setup.ec" type="1000"]
addresses : string[1] = "setup_id"


[category="rack.ec" type="1100"]
extends : string    = "setup.ec"
by      : string[1] = "rack_id"


[category="crate.ec" type="1200"]
extends : string    = "rack.ec"
by      : string[1] = "crate_id"


[category="ctrl_board.ec" type="1300"]
inherits : string = "crate.ec"


[category="feb_board.ec" type="1400"]
extends : string    = "crate.ec"
by      : string[1] = "module_id"


[category="wavecatcher_asic.ec" type="2000"]
extends : string    = "feb_board.ec"
by      : string[1] = "asic_id"


[category="wavecatcher_feb_fpga.ec" type="2100"]
extends : string    = "feb_board.ec"
by      : string[1] = "feb_fpga_id"


[category="wavecatcher_ctrl_fpga.ec" type="2200"]
inherits : string   = "feb_board.ec"


[category="trackerfeb_asic.ec" type="3000"]
extends : string    = "feb_board.ec"
by      : string[1] = "asic_id"


[category="trackerfeb_ctrl_fpga.ec" type="3100"]
inherits : string   = "feb_board.ec"


