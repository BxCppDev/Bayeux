DOXYFILE_ENCODING      = UTF-8
PROJECT_NAME           = "@PROJECT_NAME@"
PROJECT_NUMBER         = "@BayeuxOCDTutorial_VERSION@"
PROJECT_BRIEF          = "Tutorial for adding object configuration description (OCD) support to a class"
OUTPUT_DIRECTORY       = @bxocdtutorial_DOXYGEN_OUTPUT_DIR@
CREATE_SUBDIRS         = NO
OUTPUT_LANGUAGE        = English
BRIEF_MEMBER_DESC      = YES
REPEAT_BRIEF           = YES
FULL_PATH_NAMES        = NO
SHORT_NAMES            = NO
STRIP_FROM_INC_PATH    = ..
MARKDOWN_SUPPORT       = YES
BUILTIN_STL_SUPPORT    = YES
EXTRACT_ALL            = YES
EXTRACT_LOCAL_CLASSES  = YES
INPUT                  = @PROJECT_SOURCE_DIR@
FILE_PATTERNS          = */my/*.h */my/*.cc */programs/*.cxx
RECURSIVE              = YES
EXCLUDE_PATTERNS       = */.svn/*
INLINE_INFO            = YES
GENERATE_LATEX         = NO
GENERATE_MAN           = NO
